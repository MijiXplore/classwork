@{
    @model WebApplication8.Models.user;

    <form asp-action="AddUser" asp-controller="Home" method="post">
        Name:
        @Html.TextAreaFor(u => u.name);
        Email:
        @Html.TextAreaFor(u => u.email);
        Password:
        @Html.TextAreaFor(u => u.password);
        <input type="submit" value="Submit" />
    </form>
}
